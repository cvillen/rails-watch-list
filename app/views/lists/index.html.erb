<div class="container">
  <h1>Lists</h1>
  <%= cl_image_tag("cpj1kgvomk9a7o7ugyhn",
        width: 400, height: 300, crop: :fill) %>
  <div>
    <% @lists.each do |list| %>
      <ul>
        <li><%= list.name %>  <%= link_to 'Show', list %></li>
      </ul>
    <% end %>
  </div>

  <%= link_to 'Add new list', new_list_path %>
</div>
